<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wangning">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".dialog.DialogActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".launchmode.SecondActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />

        <activity android:name=".launchmode.ThirdActivity" />


        <activity
            android:name=".dialog.SignInSuccessActivity"
            android:screenOrientation="portrait"
            android:theme="@style/sign_in_detail_dialog" />


    </application>

</manifest>